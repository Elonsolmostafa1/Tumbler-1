<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/loginSignupFragment">


    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.example.tumbler.signupandin.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_loginSignupFragment"
            app:destination="@id/loginSignupFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginSignupFragment"
        android:name="com.example.tumbler.signupandin.LoginSignupFragment"
        android:label="fragment_login_signup"
        tools:layout="@layout/fragment_login_signup" >
        <action
            android:id="@+id/action_loginSignupFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_loginSignupFragment_to_loginWithEmailFragment"
            app:destination="@id/loginWithEmailFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginWithEmailFragment"
        android:name="com.example.tumbler.signupandin.LoginWithEmailFragment"
        android:label="fragment_login_with_email"
        tools:layout="@layout/fragment_login_with_email" />

</navigation>